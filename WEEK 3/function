def is_fever(temp):
    value = float(temp[:-1])   # numeric part
    unit = temp[-1]            # last character (F or C)

    if unit == "F":
        return value > 98.6
    elif unit == "C":
        return value > 37
    else:
        raise ValueError("Invalid temperature unit")

# 2. Boiling checker
def is_boiling(temp):
    value = float(temp[:-1])
    unit = temp[-1]

    if unit == "F":
        return value >= 212
    elif unit == "C":
        return value >= 100
    else:
        raise ValueError("Invalid temperature unit")

# 3. Hamming distance
def hamming_distance(str1, str2):
    if len(str1) != len(str2):
        raise ValueError("Strings must be the same length")

    return sum(c1 != c2 for c1, c2 in zip(str1, str2))
